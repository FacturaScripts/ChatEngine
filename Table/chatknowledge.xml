<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : chatknowledge.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the chatknowledge table.
-->
<table>
    <column>
        <name>answer</name>
        <type>text</type>
        <null>NO</null>
    </column>
    <column>
        <name>bannedwords</name>
        <type>character varying(200)</type>
    </column>
    <column>
        <name>certainty</name>
        <type>integer</type>
    </column>
    <column>
        <name>creationdate</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>idknowledge</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('chatknowledge_idknowledge_seq'::regclass)</default>
    </column>
    <column>
        <name>keywords</name>
        <type>character varying(200)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>chatknowledge_pkey</name>
        <type>PRIMARY KEY (idknowledge)</type>
    </constraint>
</table>